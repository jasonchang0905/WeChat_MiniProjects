<!-- 我在synway的故事 -->
<view class="header">
  <view class="title">
    <image src="../images/synLogo.png"></image>
  </view>
  <swiper class="slide" autoplay="true" indicator-dots="true" indicator-color="white" circular="true" interval="3000">
    <block wx:for="{{slides}}" wx:key="id">
      <swiper-item>
        <image src="{{item.image}}"></image>
      </swiper-item>
    </block> 
  </swiper>
</view>
<view class="content">
  <view class="content-title">关于三汇</view>
  <view class="content-title">我在三汇</view>
</view>

